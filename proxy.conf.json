{
  "/api/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "timeout": 30000,
    "headers": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  },
  "/actuator/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "timeout": 30000,
    "headers": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  }
}
